{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/user(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/league(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}